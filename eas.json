{
  "cli": {
    "version": ">= 0.55.1"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "default",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "cocoapods": "1.12.0"
      },
      "env": {
        "APP_ENV": "dev"
      },
      "cache": {
        "key": "2"
      }
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "staging"
      },
      "ios": {
        "cocoapods": "1.12.0"
      },
      "cache": {
        "key": "2"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production"
      },
      "ios": {
        "cocoapods": "1.12.0"
      },
      "cache": {
        "key": "2"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
