    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.1.0",
    "standard-version": "^9.5.0",
