{
  "name": "psychomapp",
  "version": "0.0.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "types": "tsc --noEmit",
    "test": "yarn lint && yarn types && yarn test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watchAll",
    "compile:plugins": "yarn tsc ./plugins/*.ts --skipLibCheck",
    "prebuild:patch": "npx standard-version --release-as patch",
    "prebuild:minor": "npx standard-version --release-as minor",
    "build:dev": "eas build --profile dev --platform ios",
    "update:dev": "eas update --profile dev --platform ios",
    "build:staging": "eas build --profile staging --non-interactive --platform ios",
    "update:staging": "eas update --profile staging --non-interactive --platform ios",
    "build:prod": "eas build --profile production --platform ios",
    "update:prod": "eas build --profile production --platform ios"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.1",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest-expo": "^49.0.0",
    "prettier": "^3.1.0",
    "standard-version": "^9.5.0",
    "standard-version-expo": "^1.0.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
